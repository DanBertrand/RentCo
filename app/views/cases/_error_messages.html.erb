          <%if msg == "Title can't be blank"%>
            <%msg = "Le titre du dossier doit être présent" %>
            <h4>
              <span class="text-danger"><%= msg %> </span>
            </h4>
          <%elsif msg == "Title is too short (minimum is 5 characters)"%>
            <%msg = "Le titre du dossier est trop court (5 caractères minimum)" %>
            <h4>
              <span class="text-danger"><%= msg %> </span>
            </h4>
