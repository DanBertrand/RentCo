<div id="error_messages_edit_user">
  <% if @case.errors.any? %>
    <div id="error_explanation">
      <div class="alert mb-3", style="vertical-align:top;">
        <h4><%= I18n.t("errors.messages.not_saved", count: @case.errors.count, resource: @case.class.model_name.human.downcase) %> <button type="button" class="close" data-dismiss="alert" aria-label="Close">
          <span aria-hidden="true" class="error">x</span>
        </button></h4>
